import java.util.*;

/**
 * You are given N sticks, where each stick is of positive integral length.
 * A cut operation is performed on the sticks such that all of them are reduced 
 * by the length of the smallest stick.
 * 
 * 
 * */

public class CutTheStick {
  
  public static void main(String[] args) {
   Scanner in = new Scanner(System.in);
   int n = in.nextInt();
   Map<Integer> map = new TreeMap<Integer>();
   while (n-- > 0 ) {
    int i = in.nextInt();
    if (!map.contains(i))
      map.put(i, 1);
    else {
     int count = map.get(i);
     map.put(i, count + 1);
    }
   }
   int consumed = 0;
   do {
   System.out.println(n - consumed);
   consumed += map.firstEntry().value();
   map.remove(map.firstKey());
   }
   while (!map.isEmpty());
   
   
   
   }
   
  }
}